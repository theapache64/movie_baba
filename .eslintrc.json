{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "fetch": false
  },
  "env":{
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/prefer-stateless-function": [
      0,
      {
        "ignorePureComponents": true
      }
    ],
    "no-console": "off",
    "no-alert": "off",
    "indent": "off",
    "react/jsx-indent-props": "off",
    "react/jsx-indent": "off",
    "react/prop-types": [
      "error",
      {
        "ignore": ["navigation"]
      }
    ]
  }
}
